{{page-title "Viewquestions"}}
{{outlet}}
<ul class="nav-bar">
    <li class="nav-item"><LinkTo class="nav-ele" @route="admin-dashboard">Home</LinkTo></li>
</ul>
<h1>Questions</h1>


<table>
    <tr>
        <th>Qn Id</th>
        <th>Qn Desc</th>
        
    </tr>
    {{#each @model.questions as |set|}}
        <tr>
            <td>{{set.id}}</td>
            <td><LinkTo @route="answers" @query={{hash qnid=set.id uid=@model.uid}}>{{set.question}}</LinkTo></td>
        </tr>
    {{/each}}
</table>